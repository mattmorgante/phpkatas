<? 
function firstNonRepeated($string) { 
	$array = str_split($string);
	$previous = $array[0];
	echo $previous;
	for ($i=1; $i < $array.length; $i++) {
		echo $array[$i], PHP_EOL; 
		if ($previous != $array[$i]) {
			echo $array[$i];
		} else {
			$previous = $array[$i]; 
		}
	}
	echo  "false";	
}
echo firstNonRepeated('hello');
